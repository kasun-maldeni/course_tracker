<section class="dashboard warmups">
  <h2>Warmups</h2>
  <table>
    <tr>
      <th>Task</th>
      <th>Release date</th>
      <th>Solution</th>
    </tr>
    <% @cohort_tasks.each do |ct| %>
    <tr>
      <td><%= link_to ct.task.name, ct.task.question_link, target: "_blank" %></td>
      <td><%= local_time(ct.release_date) %></td>
      <% if Time.now > ct.release_date + 2.hours %>
        <td><%= link_to 'solution', ct.task.solution_link, target: "_blank" %></td>
      <% end %>
    </tr>
    <% end %>
  </table>
</section>
