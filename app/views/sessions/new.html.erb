<section class="sessions new">
  <% flash.each do |key, value| %>
    <%= content_tag :div, value, class: "flash #{key}" %>
  <% end %>
  <main class='log-in'>
    <h2>Log in</h2>
    <%= form_tag '/sessions', method: 'post' do %>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
      <%= submit_tag "log in" %>
    <% end %>
  </main>
</section>
